<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.rainchen.filetoolstest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.rainchen.filetoolstest.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".download.SingleActivity"/>
        <activity android:name=".download.QueueActivity"/>
        <activity android:name=".download.EachBlockProgressActivity"/>
        <activity android:name=".download.ManagerActivity"/>
        <activity android:name=".download.comprehensive.ComprehensiveActivity"/>
        <activity android:name=".download.BunchActivity"/>
        <activity android:name=".download.ContentUriActivity"/>
        <activity android:name=".download.NotificationActivity"/>
        <activity android:name=".download.DownloadMainActivity"/>
        <activity android:name=".UploadActivity"/>
    </application>

</manifest>